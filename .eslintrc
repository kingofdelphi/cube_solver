{
  "extends": ["airbnb"],
  "plugins": [
    "react"
  ],
  //prevent "jsdoc required" error from google eslint
  "rules": {
    "indent": 0,
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-filename-extension": 0,
    "no-param-reassign": 0,
    "no-nested-ternary": 0,
    "no-mixed-operators": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  //prevent "import is reserved" error
  "parser": "babel-eslint",
  "env": {
    //prevent "document is not defined" error
    "browser": true
  }
}